@model IEnumerable<_710912_LOKO.BussinessEntities.Evaluacion>
@{
    ViewBag.Title = "Evaluaciones";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.ActionLink("Ver Preguntas (usando Joins)", "VerPreguntasJOINS", new { evaluacionId = item.Id })
        </td>
        <td>
            @Html.ActionLink("Ver Preguntas (usando VM)", "VerPreguntasVM", new { evaluacionId = item.Id})
        </td>
    </tr>
}

</table>
